<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="720.0" maxWidth="1280.0" minHeight="720.0"
      minWidth="1280.0"
      spacing="100.0" xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="Controllers.Student.ChosenGameMenuController">
    <children>
        <Button fx:id="playButton" mnemonicParsing="false" onMouseClicked="#playGame" text="Play"/>
        <Button fx:id="leaderBoardButton" mnemonicParsing="false" onMouseClicked="#toSubjectLeaderBoard"
                text="Leaderboard"/>
        <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#toSchoolGames" text="Back"/>
    </children>
</VBox>
