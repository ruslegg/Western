<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" prefHeight="720.0" prefWidth="1280.0"
      xmlns="http://javafx.com/javafx/8.0.141" fx:controller="Controllers.Student.QuizFinishedController">
    <children>
        <Label fx:id="congratulationLabel" alignment="CENTER" contentDisplay="CENTER" text="Congratulations">
            <font>
                <Font size="50.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="350.0"/>
            </VBox.margin>
        </Label>
        <HBox alignment="CENTER">
            <children>
                <Label alignment="CENTER" text="Score: ">
                    <font>
                        <Font size="20.0"/>
                    </font>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Label>
                <Label fx:id="scoreLabel" alignment="CENTER">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
            </children>
            <VBox.margin>
                <Insets bottom="10.0"/>
            </VBox.margin>
        </HBox>
        <HBox alignment="CENTER">
            <children>
                <Label alignment="CENTER" text="Correct answer percentage: ">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
                <Label fx:id="answerRateLabel" alignment="CENTER">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
            </children>
            <VBox.margin>
                <Insets bottom="50.0"/>
            </VBox.margin>
        </HBox>
        <Button fx:id="mainMenuButton" mnemonicParsing="false" onMouseClicked="#toMainMenu" text="Main Menu"/>
    </children>
</VBox>
