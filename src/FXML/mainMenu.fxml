<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Student.MainMenuController">
   <children>
      <Button fx:id="startButton" mnemonicParsing="false" onMouseClicked="#toTypeOfGame" text="Start" />
      <Button fx:id="teamButton" mnemonicParsing="false" onMouseClicked="#toTeam" text="Team" />
      <Button fx:id="settingsButton" mnemonicParsing="false" onMouseClicked="#toSettings" text="Settings" />
      <Button fx:id="statisticsButton" mnemonicParsing="false" onMouseClicked="#toStatistics" text="Statistics" />
      <Button fx:id="quitButton" mnemonicParsing="false" onMouseClicked="#quit" text="Quit" />
   </children>
</VBox>
