<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="Controllers.Student.MainMenuController">
  <children>
    <Button id="startButton" fx:id="competitionsButton" mnemonicParsing="false" onMouseClicked="#toTypeOfGame" text="COMPETITIONS" />
    <Button fx:id="settingsButton" mnemonicParsing="false" onMouseClicked="#toSettings" text="SETTINGS" />
    <Button fx:id="quitButton" mnemonicParsing="false" onMouseClicked="#quit" text="QUIT" />
  </children>
</VBox>
