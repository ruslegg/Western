<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1280.0" spacing="100.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SettingsController">
   <children>
      <CheckBox fx:id="soundButton" maxHeight="27.0" maxWidth="113.0" minHeight="27.0" minWidth="113.0" mnemonicParsing="false" onMouseClicked="#soundTrigger" prefHeight="27.0" prefWidth="113.0" text="Sound" />
      <CheckBox fx:id="musicButton" maxHeight="27.0" maxWidth="113.0" minHeight="27.0" minWidth="113.0" mnemonicParsing="false" onMouseClicked="#musicTrigger" prefHeight="27.0" prefWidth="113.0" selected="true" text="Music" />
      <Button fx:id="backButton" maxHeight="27.0" maxWidth="113.0" minHeight="27.0" minWidth="113.0" mnemonicParsing="false" onMouseClicked="#toMainMenu" prefHeight="27.0" prefWidth="113.0" text="Back" />
   </children>
</VBox>
