<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Student.UserTeamLessOptions">
   <children>
      <Button fx:id="createButton" mnemonicParsing="false" onMouseClicked="#toCreateTeam" text="Create" />
      <Button fx:id="joinButton" mnemonicParsing="false" onMouseClicked="#toJoinTeam" text="Join" />
      <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#toMainMenu" text="Back" />
   </children>
</VBox>
