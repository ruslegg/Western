<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>


<GridPane alignment="center" hgap="10" prefHeight="720.0" prefWidth="1920.0" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="Controllers.LoseMenuController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button fx:id="restartButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="113.0" text="RESTART" onMouseClicked="#toGame" />
      <Button fx:id="menuButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="113.0" text="MENU" GridPane.rowIndex="1" onMouseClicked="#toMainMenu"/>
   </children>
</GridPane>
