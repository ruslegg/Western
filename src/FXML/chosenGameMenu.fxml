<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Student.ChosenGameMenuController">
   <children>
      <Button fx:id="playButton" mnemonicParsing="false" onMouseClicked="#playGame" text="Play" />
      <Button fx:id="leaderBoardButton" mnemonicParsing="false" onMouseClicked="#toSubjectLeaderBoard" text="Leaderboard" />
      <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#toSchoolGames" text="Back" />
   </children>
</VBox>
