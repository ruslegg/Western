<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1280.0" spacing="50.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CompetitionIntroSceneController">
   <children>
      <Button fx:id="competitionButton" contentDisplay="CENTER" maxHeight="100.0" maxWidth="200.0" minHeight="27.0" minWidth="113.0" mnemonicParsing="false" onMouseClicked="#toStart" prefHeight="27.0" prefWidth="113.0" text="Start Competition" />
      <Button fx:id="leaderboardButton" contentDisplay="CENTER" maxHeight="100.0" maxWidth="200.0" minHeight="27.0" minWidth="113.0" mnemonicParsing="false" onMouseClicked="#toLeaderboard" prefHeight="27.0" prefWidth="113.0" text="Leaderboard" />
      <Button fx:id="backButton" contentDisplay="CENTER" maxHeight="100.0" maxWidth="200.0" minHeight="27.0" minWidth="113.0" mnemonicParsing="false" onMouseClicked="#toBack" prefHeight="27.0" prefWidth="113.0" text="Back" />
   </children>
</VBox>
