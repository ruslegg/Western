<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" fillWidth="false" minHeight="720.0" minWidth="1280.0" spacing="30.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
   <children>
    <TextField fx:id="username" minWidth="200.0" promptText="username" />
    <PasswordField fx:id="password" minWidth="200.0" promptText="password" />
    <Button fx:id="loginButton" mnemonicParsing="false" onMouseClicked="#checkCredentials" text="Login" />
      <Label onMouseClicked="#showRegisterDialog" text="Don't have an Account?" underline="true">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
   </children>
</VBox>
