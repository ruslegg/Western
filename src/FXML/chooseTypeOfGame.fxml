<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="100.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ChooseTypeOfGameController">
   <children>
      <Button fx:id="competitionButton" mnemonicParsing="false" onMouseClicked="#toCompetition" prefHeight="27.0" prefWidth="113.0" text="Competition" textAlignment="CENTER" />
      <Button fx:id="normalGameButton" mnemonicParsing="false" onMouseClicked="#toRandom" prefHeight="27.0" prefWidth="113.0" text="Normal" />
      <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#toMainMenu" prefHeight="27.0" prefWidth="113.0" text="Back" />
   </children>
</VBox>
