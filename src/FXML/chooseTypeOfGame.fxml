<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Student.ChooseTypeOfGameController">
   <children>
      <Button fx:id="competitionButton" mnemonicParsing="false" onMouseClicked="#toCompetition" text="Competition" textAlignment="CENTER" />
      <Button fx:id="normalGameButton" mnemonicParsing="false" onMouseClicked="#toRandom" text="Normal" />
      <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#toMainMenu" text="Back" />
   </children>
</VBox>
