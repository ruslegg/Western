<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" fillWidth="false" minHeight="720.0" minWidth="1280.0" spacing="25.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Student.RegisterController">
   <children>
    <TextField fx:id="firstName" minWidth="170.0" promptText="First Name" />
    <TextField id="name" fx:id="lastName" minWidth="170.0" promptText="Last Name" />
    <TextField id="user" fx:id="username" minWidth="170.0" promptText="Username" />
    <PasswordField fx:id="password" minWidth="170.0" promptText="Password">
         <VBox.margin>
            <Insets />
         </VBox.margin></PasswordField>
      <HBox alignment="CENTER" minHeight="30.0" minWidth="100.0" spacing="27.0">
         <children>
            <ComboBox fx:id="classComboBox" promptText="Class" />
            <ComboBox fx:id="classLetterComboBox" promptText="Letter" />
         </children>
      </HBox>
    <Button fx:id="signUpButton" mnemonicParsing="false" onMouseClicked="#registerAccount" text="Sign Up" />
    <Button id="backButton" mnemonicParsing="false" onMouseClicked="#showLoginDialog" text="Back" />
   </children>
</VBox>
